{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "import/no-unresolved": 0,
    "no-undef": 0,
    "react/forbid-prop-types": 0,
    "no-console": 0,
    "import/newline-after-import": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "no-new": 0
  }
}
